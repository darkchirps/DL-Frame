{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es2020",
      "dom"
    ],
    "target": "es2020",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "temp/vscode-dist",
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ]
}